        {
            "name": "Debug MyProject (Linux)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/code/MyProject",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
